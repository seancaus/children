// 桌子协议

namespace ziv.hall;

// 
table LoginRequest
{
    playerId:string;
}

table LoginReply
{
    playerId:string;
}

//心跳
table Heartbeat
{
    playerId:string;
}

union Any
{
    LoginRequest,
    LoginReply,
    Heartbeat
}

table Message
{
    id:int;
    object:Any;
}

root_type Message;

